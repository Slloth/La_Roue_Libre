<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('build/images/favicon.ico') }}" type="image/x-icon">
    </head>
    <body>
        {% include "base/nav.html.twig" %}
        <main>
        <div>
            <img id="roue1" src="{{ asset('build/images/roue-site.png') }}" alt="">
            <img id="roue2" src="{{ asset('build/images/roue-site.png') }}" alt="">
            <img id="roue3" src="{{ asset('build/images/roue-site.png') }}" alt="">
        </div>
            {% block body %}{% endblock %}
        </main>
        <img class="wave-svg" src="{{ asset('build/images/wave.svg' )}}" alt="">
        {% include "base/footer.html.twig" %}
    </body>
</html>
