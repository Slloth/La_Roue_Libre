{% extends '@EasyAdmin/page/content.html.twig' %}

{% block content_title %}Hello AdherentController!{% endblock %}

{% block main %}
    <table class="datagrid text-center">
        <thead>
            <tr>
                {% for header in records[0] %}
                    <th>{{ header|convert_encoding('UTF-8', 'ISO-8859-1') }}</th>
                {% endfor %}
            </tr>
        </thead>
        <tbody>
            {% for items in records[1] %}
                <tr>
                {% for item in items %}
                    <td>{{ item|convert_encoding('UTF-8', 'ISO-8859-1') }}</td>
                {% endfor %}
                <tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}
